<div>
  Welcome to ChitterChatter!<br>
  <a href="/peeps/new">New peep</a>

<ul>
  <% @peeps.each do |peep| %>
    <%=peep.text %><br>
    Posted by: <a href=/users/<%=(User.first(id: peep.user_id)).username %>><%= (User.first(id: peep.user_id)).username %></a>,
    <br>
    <%=(User.first(id: peep.user_id)).name%>
    at <%=peep.timestamp%><br><br>
  <% end %>
</ul>

